{
  "name": "algerian-medical-passport-blockchain",
  "version": "1.0.0",
  "description": "A blockchain-based medical passport system for Algeria",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:local": "hardhat run scripts/deploy.js --network localhost",
    "deploy:testnet": "hardhat run scripts/deploy.js --network polygonMumbai",
    "deploy:mainnet": "hardhat run scripts/deploy.js --network polygon",
    "verify": "hardhat verify --network polygonMumbai",
    "verify:testnet": "hardhat verify --network polygonMumbai",
    "verify:mainnet": "hardhat verify --network polygon",
    "coverage": "hardhat coverage",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "lint": "eslint . --ext .js,.ts,.json",
    "lint:fix": "eslint . --ext .js,.ts,.json --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md,sol}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md,sol}\"",
    "clean": "hardhat clean",
    "node": "hardhat node",
    "build": "hardhat compile"
  },
  "keywords": [
    "blockchain",
    "medical",
    "passport",
    "healthcare",
    "algeria",
    "ethereum",
    "polygon",
    "smart-contracts"
  ],
  "author": "nasrosoft",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "chai": "^4.3.7",
    "concurrently": "^8.2.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.7.2",
    "ganache": "^7.9.2",
    "hardhat": "^2.15.0",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.2",
    "dotenv": "^16.1.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/algerian-medical-passport-blockchain.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/algerian-medical-passport-blockchain/issues"
  },
  "homepage": "https://github.com/yourusername/algerian-medical-passport-blockchain#readme"
}
